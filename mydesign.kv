<Password>:
	name : 'main'
	BoxLayout:
        orientation: 'vertical'

        canvas.before:
            Rectangle:
                source: '/storage/emulated/0/story/IMG_20241018_043221_028.jpg'
                pos: self.pos
                size: self.size
	GridLayout:
		cols : 1
		GridLayout:
			rows : 3
			Label:
				text : root.arabic_text
				font_size: 80
				pos_hint : {'x': 0.5 , 'y': 0.9}
				font_name : 'Amiri-Bold.ttf'
			#Image:
				#source: '/storage/emulated/0/Pictures/Telegram/IMG_20241018_043221_028.jpg'
				#pos_hint : {'x': 0.5 , 'y': 0.8}
			Label:
				text : ""
				font_size: 80
				color: (1,0,0,180/255.0)
				size_hint: (0.1 , 0.3)
				font_name : 'Amiri-Bold.ttf'
			TextInput:
				id: pasa
				hint_text: 'Password'
				multiline: False
				background_color: (0,0,0,100/255.0)
				size_hint : (0.1 , 0.2)
		Button:
			text: root.arabik_text
			font_size: 50
			font_name : 'Amiri-Bold.ttf'
			size_hint : (0.1 , 0.2)
			background_color: (1,1,1,180/255.0)
			on_release:
				app.root.current = 'storys' if pasa.text == 'roro' else 'error'
				root.manager.transition.direction = 'up' 
